                                        &amp;lt;td&amp;gt;{{ $item-&amp;gt;id }}&amp;lt;/td&amp;gt;

                                        &amp;lt;td&amp;gt;{{ $item-&amp;gt;nome }}&amp;lt;/td&amp;gt;&amp;lt;td&amp;gt;{{ $item-&amp;gt;cpf }}&amp;lt;/td&amp;gt;&amp;lt;td&amp;gt;{{ $item-&amp;gt;nascimento }}&amp;lt;/td&amp;gt;

                                        &amp;lt;td&amp;gt;

                                            &amp;lt;a href=&quot;http://backup/portal/{{ url('/clientes/' . $item-&amp;gt;id) }}&quot; title=&quot;View Cliente&quot;&amp;gt;&amp;lt;button class=&quot;btn btn-info btn-xs&quot;&amp;gt;&amp;lt;i class=&quot;glyphicon glyphicon-eye-open&quot;&amp;gt;&amp;lt;/i&amp;gt; View&amp;lt;/button&amp;gt;&amp;lt;/a&amp;gt;

                                            &amp;lt;a href=&quot;http://backup/portal/{{ url('/clientes/' . $item-&amp;gt;id . '/edit') }}&quot; title=&quot;Edit Cliente&quot;&amp;gt;&amp;lt;button class=&quot;btn btn-primary btn-xs&quot;&amp;gt;&amp;lt;i class=&quot;glyphicon glyphicon-edit&quot;&amp;gt;&amp;lt;/i&amp;gt; Edit&amp;lt;/button&amp;gt;&amp;lt;/a&amp;gt;



                                            &amp;lt;form

                                                method ='DELETE',

                                                action = &quot;'/clientes', $item-&amp;gt;id&quot;

                                                style = 'display:inline'

